section#location.section.section-location(data-track="view", data-track-category="Section", data-track-label="#{labels.sections[1].label}")

  .wrapper.text-center

    h1.section-title
      span #{labels.sections[1].label}

    .section-text
      p #{about.location.place} - #{about.location.city}, #{about.location.state}

  .grid

    .location-map.width-1-1.width-lg-2-4
      iframe(width="100%", height="100%", frameborder="0", style="border:0", src="https://www.google.com/maps/embed/v1/place?q=#{about.location.place}&key=#{about.location.mapsKey}")
